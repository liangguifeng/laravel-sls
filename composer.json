{
    "name" : "seffeng/laravel-sls",
    "description" : "PHP laravel extension sls",
    "keywords" : [
        "PHP",
        "laravel",
        "lumen",
        "seffeng",
        "sls"
    ],
    "license" : "MIT",
    "homepage" : "https://github.com/seffeng/laravel-sls",
    "authors" : [{
            "name" : "seffeng",
            "email" : "save.zxf@gmail.com",
            "homepage" : "https://github.com/seffeng"
        }
    ],
    "require" : {
        "php" : ">=7.1.3",
        "illuminate/log" : "^5.5|^6.0|^7.0|^8.0",
        "seffeng/aliyun-sls": "^0.1.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^9.1"
    },
    "autoload" : {
        "psr-4" : {
            "Seffeng\\LaravelSLS\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Seffeng\\LaravelSLS\\Tests\\" : "tests/"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Seffeng\\LaravelSLS\\SLSServiceProvider"
            ]
        }
    },
    "minimum-stability" : "stable"
}